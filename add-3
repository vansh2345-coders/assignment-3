#include <iostream>
#include <stack>
using namespace std;

void printNGE(int arr[], int n) {
    stack<int> st;

    for (int i=0;i<n;i++){
        while (!st.empty() && arr[i] > st.top()) {
            cout << "Next greater of"<<st.top()<<" is"<<arr[i]<<endl;
            st.pop();
        }
        st.push(arr[i]);
    }
    while (!st.empty()) {
        cout<<"Next greater of "<<st.top()<<" is -1"<< endl;
        st.pop();
    }
}
int main(){
    int arr[]={4, 5, 2, 25};
    int n=sizeof(arr) / sizeof(arr[0]);
    printNGE(arr, n);
    return 0;
}
