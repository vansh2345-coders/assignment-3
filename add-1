#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter size of array: ";
    cin>>n;
    int arr[n];
    cout<<"Enter elements: ";
    for(int i=0;i<n;i++) cin>> arr[i];
    stack<int> st;
    cout<<"Nearest Smaller Elements: ";
    for(int i=0;i<n;i++){
        while(!st.empty() && st.top()>= arr[i]){
            st.pop();
        }
        if(st.empty())
            cout<<-1<<" ";
        else
            cout<<st.top()<<" ";
        st.push(arr[i]);
    }
    cout<<endl;
    return 0;
}
